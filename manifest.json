{
  "name": "GetCheggGo",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Automate My Chegg Skip",
  "homepage_url": "https://github.com/mahi424",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icon.jpg",
    "default_title": "GetCheggGo"
  },
  "permissions": [
    "https://www.chegg.com/*"
  ]
}
